    "# conda install -c conda-forge jdatetime\n",
    "import jdatetime\n",
    "\n",
    "def date_difference(s, t):\n",
    "    date1 = jdatetime.datetime.strptime(s, '%Y/%m/%d')\n",
    "    date2 = jdatetime.datetime.strptime(t, '%Y/%m/%d')\n",
    "\n",
    "    return (date1 - date2).days\n"
   ]
